{
  "manifestVersion": 5,
  "id": "com.brandon.timetracker",
  "name": "Brandon Timer",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PPRO",
      "minVersion": "24.0.0"
    },
    {
      "app": "AEFT",
      "minVersion": "24.0.0"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "brandon-timer",
      "label": { "default": "Brandon Timer" },
      "minimumSize": { "width": 300, "height": 200 },
      "icon": "icon.png",
      "menuItems": []
    }
  ]
,
  "requiredPermissions": {
    "network": { "domains": "all" },
    "clipboard": "readAndWrite",
    "launchProcess": { "schemes": ["https"] }
  }
}
